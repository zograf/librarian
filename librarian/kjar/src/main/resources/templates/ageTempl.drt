template header
age
flag
sal

package rules;
import com.librarian.model.*;

template "age rules"

rule "Age @{row.rowNumber}"
salience @{sal}
no-loop
    when
        $u: RecommendingPreferences(up.age > @{age})
        not Flag(name == "flag_adult", rp == $u)
        not Flag(name == "flag_juvenile", rp == $u)
        not Flag(name == "flag_ya", rp == $u)
    then
        insert(new Flag(@{flag}, $u));
end

end template